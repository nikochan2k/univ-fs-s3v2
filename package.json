{
  "name": "univ-fs-s3v2",
  "version": "0.5.0",
  "description": "The universal Filesystem API for S3 SDK v2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "fs",
    "filesystem",
    "storage",
    "isomorphic",
    "universal",
    "s3"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "compile": "tsc",
    "build": "npm-run-all clean compile",
    "npm-version": "npm version patch",
    "npm-publish": "npm publish",
    "release": "npm-run-all clean compile npm-version npm-publish",
    "test": "jest --runInBand",
    "web-clean": "rimraf ./dist",
    "web-compile": "webpack",
    "web-build": "npm-run-all web-clean web-compile",
    "karma": "karma start",
    "web-test": "npm-run-all web-build karma"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.spec.+(ts|tsx|js)"
    ],
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "jasmine": "^3.10.0",
    "jest": "^27.3.1",
    "karma": "^6.3.8",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.63.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "aws-sdk": "^2.1025.0",
    "tslib": "^2.3.1",
    "univ-fs": "^0.5.32"
  },
  "author": "Yoshihiro Seki",
  "license": "MIT"
}
