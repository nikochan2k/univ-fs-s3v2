{
  "name": "univ-fs-s3v2",
  "version": "0.6.2",
  "description": "The universal Filesystem API for S3 SDK v2",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "fs",
    "filesystem",
    "storage",
    "isomorphic",
    "universal",
    "s3"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "compile": "tsc",
    "build": "npm-run-all clean compile",
    "npm-version": "npm version patch",
    "npm-publish": "npm publish",
    "release": "npm-run-all clean compile npm-version npm-publish",
    "test": "jest --runInBand",
    "web-clean": "rimraf ./dist",
    "web-compile": "webpack",
    "web-build": "npm-run-all web-clean web-compile",
    "karma": "karma start",
    "web-test": "npm-run-all web-build karma"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.spec.+(ts|tsx|js)"
    ],
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "jasmine": "^4.0.2",
    "jest": "^27.5.1",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "aws-sdk": "^2.1101.0",
    "tslib": "^2.3.1",
    "univ-fs": "^0.6.26"
  },
  "author": "Yoshihiro Seki",
  "license": "MIT"
}
